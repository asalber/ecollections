#################################################################
#
#    Object Properties
#
#################################################################


###  http://purl.org/ceu/lco#hasContent

:hasContent rdf:type owl:ObjectProperty ;
            
            rdfs:subPropertyOf owl:topObjectProperty .



###  http://purl.org/ceu/lco#hasElement

:hasElement rdf:type owl:AsymmetricProperty ,
                     owl:ObjectProperty ;
            
            rdfs:domain :Collection ;
            
            owl:inverseOf :isElementOf ;
            
            owl:propertyChainAxiom ( :hasItem
                                     :hasContent
                                   ) .



###  http://purl.org/ceu/lco#hasFirstItem

:hasFirstItem rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;
              
              rdfs:subPropertyOf :hasOrderedItem ;
              
              rdfs:range [ rdf:type owl:Class ;
                           owl:intersectionOf ( :OrderedItem
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasPreviousItem ;
                                                  owl:cardinality "0"^^xsd:nonNegativeInteger
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasIndex ;
                                                  owl:hasValue "1"^^xsd:positiveInteger
                                                ]
                                              )
                         ] .



###  http://purl.org/ceu/lco#hasItem

:hasItem rdf:type owl:ObjectProperty .



###  http://purl.org/ceu/lco#hasLastItem

:hasLastItem rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:subPropertyOf :hasOrderedItem ;
             
             rdfs:range [ rdf:type owl:Class ;
                          owl:intersectionOf ( :OrderedItem
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :hasNextItem ;
                                                 owl:cardinality "0"^^xsd:nonNegativeInteger
                                               ]
                                             )
                        ] .



###  http://purl.org/ceu/lco#hasNextItem

:hasNextItem rdf:type owl:AsymmetricProperty ,
                      owl:FunctionalProperty ,
                      owl:InverseFunctionalProperty ,
                      owl:IrreflexiveProperty ,
                      owl:ObjectProperty ;
             
             rdfs:range :OrderedItem ;
             
             rdfs:domain :OrderedItem ;
             
             owl:inverseOf :hasPreviousItem ;
             
             rdfs:subPropertyOf :isFollowedBy .



###  http://purl.org/ceu/lco#hasOrderedItem

:hasOrderedItem rdf:type owl:ObjectProperty ;
                
                rdfs:domain :OrderedCollection ;
                
                rdfs:range :OrderedItem ;
                
                rdfs:subPropertyOf :hasItem ;
                
                owl:propertyChainAxiom ( :hasOrderedItem
                                         :hasNextItem
                                       ) .



###  http://purl.org/ceu/lco#hasPreviousItem

:hasPreviousItem rdf:type owl:AsymmetricProperty ,
                          owl:FunctionalProperty ,
                          owl:InverseFunctionalProperty ,
                          owl:IrreflexiveProperty ,
                          owl:ObjectProperty ;
                 
                 rdfs:range :OrderedItem ;
                 
                 rdfs:domain :OrderedItem ;
                 
                 rdfs:subPropertyOf :isPrecededBy .



###  http://purl.org/ceu/lco#isElementOf

:isElementOf rdf:type owl:AsymmetricProperty ,
                      owl:ObjectProperty ;
             
             rdfs:range :Collection .



###  http://purl.org/ceu/lco#isFollowedBy

:isFollowedBy rdf:type owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ,
                       owl:ObjectProperty ,
                       owl:TransitiveProperty .



###  http://purl.org/ceu/lco#isPrecededBy

:isPrecededBy rdf:type owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ,
                       owl:ObjectProperty ,
                       owl:TransitiveProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://purl.org/ceu/lco#hasCardinality

:hasCardinality rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                
                rdfs:domain :Collection ;
                
                rdfs:range xsd:nonNegativeInteger .



###  http://purl.org/ceu/lco#hasIndex

:hasIndex rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          
          rdfs:domain :OrderedItem ;
          
          rdfs:range xsd:positiveInteger .





